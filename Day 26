#delete-nodes-from-linked-list-present-in-array:-


class Solution:
    def modifiedList(self, nums: List[int], head: Optional[ListNode]) -> Optional[ListNode]:
        remove_set = set(nums)
        dummy = ListNode(0)
        dummy.next = head
        curr = dummy
        while curr.next:
            if curr.next.val in remove_set:
                curr.next = curr.next.next
            else:
                curr = curr.next
        return dummy.next

